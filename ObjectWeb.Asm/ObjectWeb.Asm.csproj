<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>9.4.0.1</Version>
    <Authors>INRIA, France Telecom;NickAc</Authors>
    <Company />
    <Description>A very small and fast Java bytecode manipulation framework</Description>
    <Copyright></Copyright>
    <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/CursedJvmSharp/ObjectWeb.Asm/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <LangVersion>default</LangVersion>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DebugType>full</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>

</Project>